<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 指定工作空间，要与接口名相同，源代码没有去看，猜测应该是通过"这里的namespace.下边方法的id"来定位方法的 -->
<mapper namespace="com.gugu.gugumodel.mapper.TeacherMapper">
    <delete id="deleteTeacherById" parameterType="java.lang.Long">
        delete from teacher where id=#{arg0}
    </delete>

    <select id="getTeachers" resultType="com.gugu.gugumodel.pojo.entity.TeacherEntity">
        select teacher.id id,teacher.account account,teacher.teacher_name teacherName,teacher.email email
        from teacher
    </select>
</mapper>