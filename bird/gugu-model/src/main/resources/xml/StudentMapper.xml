<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 指定工作空间，要与接口名相同，源代码没有去看，猜测应该是通过"这里的namespace.下边方法的id"来定位方法的 -->
<mapper namespace="com.gugu.gugumodel.mapper.StudentMapper">
    <resultMap id="studentResultList" type="com.gugu.gugumodel.pojo.entity.StudentEntity">
        <id column="id" property="id"/>
        <result column="account" property="account"/>
        <result column="password" property="password"/>
    </resultMap>

    <delete id="deleteStudentById" parameterType="java.lang.Long">
        delete from student where id=#{arg0}
    </delete>

    <select id="searchStudent">

    </select>
</mapper>